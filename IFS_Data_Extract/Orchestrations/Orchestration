       "KD_FIRST_PF_ORDER_DATE"."FIRSTORDERDATE",
       "KD_FIRST_SCF_ORDER_DATE"."FIRSTORDERDATE" AS "FIRSTORDERDATESCG"
  FROM (((((("IFSAPP"."KD_SALES_DATA_REQUEST" "KD_SALES_DATA" LEFT OUTER JOIN
        "IFSAPP"."SALES_PART" "SALES_PART"
        ON("KD_SALES_DATA"."CATALOG_NO" = "SALES_PART"."CATALOG_NO") AND
        ("KD_SALES_DATA"."SITE" = "SALES_PART"."CONTRACT")) LEFT OUTER JOIN
        "IFSAPP"."PERSON_INFO" "PERSON_INFO" ON
        "KD_SALES_DATA"."SALESMAN_CODE" = "PERSON_INFO"."PERSON_ID") LEFT
        OUTER JOIN "IFSAPP"."CUSTOMER_ORDER" "CUSTOMER_ORDER" ON
        "KD_SALES_DATA"."ORDER_NO" = "CUSTOMER_ORDER"."ORDER_NO") LEFT
        OUTER JOIN "IFSAPP"."KD_COST_VALUES" "KD_COST_VALUES" ON
        "KD_SALES_DATA"."CATALOG_NO" = "KD_COST_VALUES"."PART_NO") LEFT
        OUTER JOIN "IFSAPP"."CUSTOMER_INFO" "CUSTOMER_INFO" ON
        "KD_SALES_DATA"."ASSOCIATION_NO" = "CUSTOMER_INFO"."CUSTOMER_ID")
        INNER JOIN "IFSAPP"."KD_FIRST_PF_ORDER_DATE"
        "KD_FIRST_PF_ORDER_DATE"
        ON("KD_SALES_DATA"."CUSTOMER_NO" =
           "KD_FIRST_PF_ORDER_DATE"."CUSTOMER_NO") AND
        ("KD_SALES_DATA"."PART_PRODUCT_FAMILY" =
        "KD_FIRST_PF_ORDER_DATE"."PART_PRODUCT_FAMILY"))
  LEFT OUTER JOIN "IFSAPP"."KD_FIRST_SCF_ORDER_DATE" "KD_FIRST_SCF_ORDER_DATE"
    ON ("KD_SALES_DATA"."CUSTOMER_NO" =
       "KD_FIRST_SCF_ORDER_DATE"."CUSTOMER_NO")
   AND ("KD_SALES_DATA"."SECOND_COMMODITY" =
       "KD_FIRST_SCF_ORDER_DATE"."SECOND_COMMODITY")
 WHERE EXTRACT (YEAR FROM "KD_SALES_DATA"."INVOICEDATE") = ?
 ORDER BY "KD_SALES_DATA"."REGION_CODE",
          "KD_SALES_DATA"."SALESMAN_CODE",
          "KD_SALES_DATA"."CUSTOMER_NO",
          "KD_SALES_DATA"."PART_PRODUCT_FAMILY",
          "KD_SALES_DATA"."PART_PRODUCT_CODE",
          "KD_SALES_DATA"."INVOICEDATE",
          "KD_SALES_DATA"."INVOICE_ID",
          "KD_SALES_DATA"."ITEM_ID"]]></string><![CDATA[
